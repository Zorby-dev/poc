{
  "name": "pasm",
  "displayName": "PASM",
  "description": "syntax highlighting for the pasm language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pasm",
      "aliases": ["pasm"],
      "extensions": [".pasm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pasm",
      "scopeName": "source.pasm",
      "path": "./syntaxes/pasm.tmLanguage.json"
    }]
  }
}
